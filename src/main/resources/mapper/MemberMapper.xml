<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.midas2018.root.repository.MemberMapper">

    <insert id="memberRegister" parameterType="com.midas2018.root.model.Member">
        INSERT INTO
        member(email, password, name)
        value(#{email}, #{password}, #{name})
    </insert>

</mapper>
